{
  "name": "discord-logger",
  "version": "1.1.0",
  "description": "A powerful Discord webhook logger with GitHub file storage for large content. Works in both Node.js and browser environments.",
  "main": "src/index.js",
  "module": "src/index.esm.js",
  "browser": "dist/discord-logger.min.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.esm.js",
      "require": "./src/index.js",
      "browser": "./dist/discord-logger.min.js",
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "src/",
    "dist/",
    "types/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:browser",
    "build:cjs": "node scripts/build-cjs.js",
    "build:esm": "node scripts/build-esm.js",
    "build:browser": "webpack --config webpack.config.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ --fix",
    "validate": "npm run lint",
    "prepublishOnly": "npm run build && npm run validate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DTDucas/discord-logger.git"
  },
  "keywords": [
    "discord",
    "webhook",
    "logger",
    "logging",
    "github",
    "storage",
    "browser",
    "nodejs",
    "universal"
  ],
  "author": {
    "name": "Duong Tran Quang - DTDucas",
    "email": "baymax.contact@gmail.com",
    "url": "https://github.com/DTDucas"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DTDucas/discord-logger/issues"
  },
  "homepage": "https://github.com/DTDucas/discord-logger#readme",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/plugin-transform-runtime": "^7.23.0",
    "@babel/runtime": "^7.23.0",
    "@playwright/test": "^1.40.0",
    "@types/jest": "^29.5.5",
    "audit-ci": "^6.6.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "buffer": "^6.0.3",
    "browserify-zlib": "^0.2.0",
    "core-js": "^3.33.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.50.0",
    "https-browserify": "^1.0.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.3",
    "util": "^0.12.5",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
